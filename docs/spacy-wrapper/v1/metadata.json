{
  "name": "CLAMS wrapper for spaCy NLP",
  "description": "Apply spaCy NLP to all text documents in a MMIF file.",
  "app_version": "v1",
  "mmif_version": "0.5.0",
  "analyzer_version": "3.1.2",
  "app_license": "Apache 2.0",
  "analyzer_license": "MIT",
  "identifier": "http://apps.clams.ai/spacy-wrapper/v1",
  "url": "https://github.com/clamsproject/app-spacy-nlp",
  "input": [
    {
      "at_type": "http://mmif.clams.ai/vocabulary/TextDocument/v1",
      "properties": {},
      "required": true
    },
    {
      "at_type": "http://vocab.lappsgrid.org/Token",
      "properties": {},
      "required": false
    }
  ],
  "output": [
    {
      "at_type": "http://vocab.lappsgrid.org/Token",
      "properties": {}
    },
    {
      "at_type": "http://vocab.lappsgrid.org/Token#pos",
      "properties": {}
    },
    {
      "at_type": "http://vocab.lappsgrid.org/Token#lemma",
      "properties": {}
    },
    {
      "at_type": "http://vocab.lappsgrid.org/NounChunk",
      "properties": {}
    },
    {
      "at_type": "http://vocab.lappsgrid.org/Sentence",
      "properties": {}
    },
    {
      "at_type": "http://vocab.lappsgrid.org/NamedEntity",
      "properties": {}
    }
  ],
  "parameters": [
    {
      "name": "pretokenized",
      "description": "Boolean parameter to set the app to use existing tokenization, if available, for text documents for NLP processing. Useful to process ASR documents, for example.",
      "type": "boolean",
      "choices": null,
      "default": 0,
      "multivalued": false
    }
  ],
  "dependencies": null,
  "more": null
}